{
  "name": "toonflixapi",
  "version": "1.0.0",
  "description": "Toonflix API server",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "start": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build": "tsc src/index.ts",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint --fix **/*.ts",
    "migrate": "npx prisma migrate save --create-db --experimental",
    "migrate_up": "npx prisma migrate up --create-db --experimental",
    "generate": "npx prisma generate",
    "introspect": "npx prisma introspect",
    "codegen": "graphql-codegen"
  },
  "engines": {
    "node": "12.18.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2018os/ToonflixAPI.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/2018os/ToonflixAPI/issues"
  },
  "homepage": "https://github.com/2018os/ToonflixAPI#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "1.17.8",
    "@prisma/cli": "^2.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.14",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "@prisma/client": "^2.1.3",
    "apollo-server-core": "^2.17.0",
    "apollo-server-express": "^2.15.0",
    "apollo-server-testing": "^2.17.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "graphql": "^15.2.0",
    "graphql-connection-resolver": "^1.0.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "typescript": "^3.9.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  }
}
